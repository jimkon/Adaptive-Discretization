    "def nodes_at_level(level, dims):\n",
    "    if level < 0:\n",
    "        return 0\n",
    "    return 2**(level*dims)+nodes_at_level(level-1, dims)\n",
    "l = []\n",
    "for dims in range(1, 6):\n",
    "    l.append(list(nodes_at_level(level, dims) for level in range(5)))\n",
    "l"
